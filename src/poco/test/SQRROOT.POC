
int sqr_root(long i)
{
long mask;
long t;
long	result;
long	lolly;


mask = 1;
for (lolly = i; mask < lolly; mask = mask << 1)
	{
	lolly = lolly >> 1;
	}

result = 0;

while (mask)
	{
	t = result | mask;
	if ((t * t) <= i)
	    result = t;
	mask = mask >> 1;
	}

return (result);
}

main()
{
long i;

for (i=1; i<1000; i = i+10)
	printf("%3ld  %d\n", i,  sqr_root(i) );
}
