String num_string(int num)
{
char buf[20];

sprintf(buf,"%d", num);
return(buf);
}

main()
{
String a = "I am a ";
String b = "wooli!";
String array[10];
int i;
String huge;

for (i=0; i<10; ++i)
	array[i] = a+b+" "+num_string(i);
for (i=0; i<10; ++i)
	huge = huge + array[i] + "...\n";
puts(huge);
}
