main()
{
	Screen *cel = GetCelScreen();
	char mask[8];
	int bit = 0x80;
	int x,y;

	for (x=0; x<8; ++x) {
		for (y=0; y<8; ++y) {
			if (GetPixel(cel,x,y))
				mask[y] |= bit;
		}
		bit >>= 1;
	}
	Qtext("%2x %2x %2x %2x %2x %2x %2x %2x", 
		mask[0], mask[1], mask[2], mask[3],
		mask[4], mask[5], mask[6], mask[7]);
}
			