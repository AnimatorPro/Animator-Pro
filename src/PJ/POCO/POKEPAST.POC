void poke_past(int x[4])
{
	x[0] = 10;
	x[1] = 11;
	x[2] = 12;
	x[3] = 13;
}

void pt_ok(int *x)
{
	x[0] = 10;
	x[1] = 11;
	x[2] = 12;
	x[3] = 13;
}

main()
{
	int array[4];
	int thousand = 1000;

	printf("thousand is %d\n", thousand);
	pt_ok(array);
	printf("thousand is %d\n", thousand);
	poke_past(array);
	printf("thousand is %d\n", thousand);
}


